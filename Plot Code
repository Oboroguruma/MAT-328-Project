plt.xlabel("Stress Level")
plt.ylabel("People")
plt.title("Dry Eye Disease by Stress level")

plt.figure(figsize=(15, 3))
sns.countplot(x = "Weight", hue = "Dry Eye Disease", data = sample)
plt.xlabel("Weight")
plt.ylabel("People")
plt.title("Dry Eye Disease by Weight Groups")

